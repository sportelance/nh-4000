{
  "name": "nh-4000-footers-tracker",
  "version": "1.0.0",
  "description": "Interactive web application for tracking hikes on New Hampshire's 48 4000-foot mountains",
  "main": "index.html",
  "scripts": {
    "start": "live-server --port=8000 --open=/index.html --watch=.",
    "dev": "live-server --port=8000 --open=/index.html --watch=. --entry-file=index.html",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write \"**/*.{js,html,css,json,md}\"",
    "serve": "python3 -m http.server 8000"
  },
  "keywords": [
    "new-hampshire",
    "4000-footers",
    "hiking",
    "mountains",
    "tracker",
    "interactive-map"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "live-server": "^1.2.2",
    "prettier": "^3.2.5"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/nh-4000-footers-tracker.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/nh-4000-footers-tracker/issues"
  },
  "homepage": "https://github.com/yourusername/nh-4000-footers-tracker#readme"
} 